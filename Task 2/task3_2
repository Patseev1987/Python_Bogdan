# if my_list.__contains__(number_the_nearest):
#     print(number_the_nearest)
# elif number_the_nearest > number:
#     print(number)
# else:
#     print(1)

print("\n"*100)

my_list = input("Enter numbers: ").split()
counter = 0
print(my_list)
number_the_nearest = int(input("Enter the number to find the nearest: "))
if my_list.__contains__(number_the_nearest):
    print(number_the_nearest)
elif number_the_nearest > 0:
    new_list = [abs(number_the_nearest-int(my_list[i]))
                for i in range(len(my_list))]
    print(number_the_nearest-min(new_list))
else:
    new_list = [abs(number_the_nearest-int(my_list[i]))
                for i in range(len(my_list))]
    print(number_the_nearest+min(new_list))
